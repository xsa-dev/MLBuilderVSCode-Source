{
	"name": "ml-net-model-builder",
	"displayName": "ML.NET Model Builder (Preview)",
	"description": "ML.NET Model Builder for VSCode",
	"publisher": "xsa-dev",
	"repository": "https://github.com/xsa-dev/MLBuilderVSCode-Source",
	"version": "0.1.1",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"AutoML",
		"ML.NET",
		"ASP.NET",
		".NET Core",
		"dotnet",
		"Model Builder"
	],
	"activationEvents": [
		"onCommand:extension.mlbuilder"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.mlbuilder",
				"title": "ML.NET: Model Builder (Preview)"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.47.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.20.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {
		"firstline": "^2.0.2"
	}
}
